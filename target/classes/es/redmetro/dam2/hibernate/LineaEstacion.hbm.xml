<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="es.redmetro.dam2.vo.LineaEstacion" table="T_LINEA_ESTACION">
    	
    	<composite-id>
			<key-property name="linea" column="cod_linea"/>
			<key-property name="estacion" column="cod_estacion"/>
		</composite-id>
		
        
        <property name="ordenM" column="orden" type="java.lang.Integer"/>
    </class>
</hibernate-mapping>